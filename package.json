{
    "name": "stargaze-cinema-react",
    "version": "1.0.0",
    "private": true,
    "license": "ISC",
    "author": "Paul Litovka",
    "description": "Website for a Stargaze Cinema network",
    "scripts": {
        "prepare": "husky install",
        "start:development": "vite dev",
        "start:production": "npm run build && npm start",
        "start": "vite preview",
        "build": "npm run typecheck && vite build",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "format": "prettier --loglevel warn --write src",
        "lint": "eslint src  --quiet --fix --ext js,ts,tsx"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.nixdev.co/litovka/stargaze-cinema-react.git"
    },
    "bugs": {
        "url": "https://gitlab.nixdev.co/litovka/stargaze-cinema-react/issues"
    },
    "homepage": "https://gitlab.nixdev.co/litovka/stargaze-cinema-react#readme",
    "keywords": [
        "react",
        "vite",
        "sass",
        "typescript",
        "vitejs",
        "reactjs",
        "nodejs",
        "eslint"
    ],
    "dependencies": {
        "@tanstack/react-location": "^3.7.4",
        "axios": "^0.26.1",
        "dayjs": "^1.11.1",
        "js-cookie": "^3.0.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-helmet": "^6.1.0",
        "react-query": "^4.0.0-beta.3",
        "swiper": "^8.1.3"
    },
    "devDependencies": {
        "@types/js-cookie": "^3.0.1",
        "@types/node": "^17.0.25",
        "@types/react": "^18.0.5",
        "@types/react-dom": "^18.0.1",
        "@types/react-helmet": "^6.1.5",
        "@typescript-eslint/eslint-plugin": "5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "@vitejs/plugin-react": "^1.3.1",
        "eslint": "8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.4.0",
        "husky": "^7.0.4",
        "prettier": "^2.6.2",
        "sass": "^1.50.1",
        "typescript": "^4.6.3",
        "vite": "^2.9.5"
    }
}
